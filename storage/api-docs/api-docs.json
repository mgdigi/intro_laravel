                "tags": [
                    "Comptes"
                ],
                "summary": "Mettre à jour un compte",
                "description": "Met à jour les informations d'un compte bancaire existant",
                "operationId": "5293c1a3f2cd81cb7294db8426c64ac7",
                "parameters": [
                    {
                        "name": "compte",
