                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "titulaire": {
                                        "type": "string",
                                        "maxLength": 150,
                                        "example": "Jean Dupont"
                                    },
                                    "informationsUser": {
                                        "properties": {
                                            "nom": {
                                                "type": "string",
                                                "maxLength": 50,
                                                "example": "Dupont"
                                            },
                                            "prenom": {
                                                "type": "string",
                                                "maxLength": 70,
                                                "example": "Jean"
                                            },
                                            "email": {
                                                "type": "string",
                                                "format": "email",
                                                "example": "jean.dupont@email.com"
                                            },
                                            "telephone": {
                                                "type": "string",
                                                "example": "+221771234567"
                                            },
                                            "nci": {
                                                "type": "string",
                                                "example": "1234567890123456"
                                            },
                                            "adresse": {
                                                "type": "string",
                                                "example": "Dakar, Sénégal"
                                            },
                                            "password": {
                                                "type": "string",
                                                "minLength": 6,
                                                "example": "newpassword123"
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Compte mis à jour avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Compte mis à jour avec succès"
                                        },
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "string",
                                                    "format": "uuid"
                                                },
                                                "numeroCompte": {
                                                    "type": "string"
                                                },
                                                "titulaire": {
                                                    "type": "string"
                                                },
                                                "type": {
                                                    "type": "string"
                                                },
                                                "solde": {
                                                    "type": "number"
                                                },
                                                "devise": {
                                                    "type": "string"
                                                },
                                                "dateCreation": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                },
                                                "statut": {
                                                    "type": "string"
                                                },
                                                "metadata": {
                                                    "properties": {
                                                        "derniereModification": {
                                                            "type": "string",
                                                            "format": "date-time"
                                                        },
                                                        "version": {
                                                            "type": "integer"
                                                        }
                                                    },
                                                    "type": "object"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Compte non trouvé",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "error"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Compte non trouvé"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
